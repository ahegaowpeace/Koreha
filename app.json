{
  "expo": {
    "name": "Koreha",
    "slug": "Koreha",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Koreha",
      "infoPlist": {
        "NSCameraUsageDescription": "撮影する写真から画像検索を行うためにアクセス許可が必要です",
        "NSPhotoLibraryUsageDescription": "フォトライブラリの写真から画像検索を行うためにアクセス許可が必要です"
      }
    }
  }
}
