{
  "expo": {
    "name": "Koreha",
    "slug": "Koreha",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Koreha",
      "infoPlist": {
        "NSCameraUsageDescription": "Use the camera to upload images and perform image searches.",
        "NSPhotoLibraryUsageDescription": "Use the photo library to upload images and perform image searches."
      }
    }
  }
}
